<div class="panel panel-primary">
    <div class="panel-heading">Coordonnées & contact <span class="en_cadre"> / Address & contact</span>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-4">
                <div class="form-group">
                    <label for="adresses">Adresse</label> /
                    <span class="en">Adresse</span>
                    <span class="label label-danger">Requis</span>
                    <input type="text" class="form-control required" id="adresse"
                           name="adresse"
                           placeholder="Votre adresse" data-validation="required"
                           data-validation-error-msg="Veuillez remplir le champ adresse"/>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label for="complement">Complément d'adresse</label> /
                    <span class="en">Complement</span>
                    <input type="text" class="form-control" id="complement"
                           name="complement"
                           data-validation-optional="true"
                           data-validation="custom"
                           data-validation-regexp="^([a-zA-Z0-9-\'\séêèëààâäôœöûüùîïç]+)$"
                           placeholder="Bâtiment, étage, appartement ..."
                           data-validation-error-msg="Veuillez remplir le champ complément d'adresse"/>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label for="code_postal">Code postal</label> / <span class="en">Zip code</span>
                    <span class="label label-danger">Requis</span>
                    <input type="text" class="form-control required" id="code_postal"
                           name="code_postal"
                           placeholder="Votre code postal" data-validation="custom"
                           data-validation-regexp="(^[0-9]{5}$)"
                           data-validation-error-msg="Veuillez entrer un code postal valide"/>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label for="ville">Ville</label> / <span class="en">City</span>
                    <span class="label label-danger">Requis</span>
                    <input type="text" class="form-control required" id="ville"
                           name="ville"
                           placeholder="Votre ville"
                           data-validation="custom"
                           data-validation-regexp="^([a-zA-Z-\'\séêèëààâäôœöûüùîïç]+)$"
                           data-validation-error-msg="Veuillez remplir le champ ville"/>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <div class="form-group">
                    <label for="fixe">Téléphone fixe</label> / <span class="en">Phone number</span>
                    <input type="tel" class="form-control" id="fixe" name="fixe"
                           placeholder="Votre numéro" data-validation-optional="true"
                           data-validation="custom"
                           data-validation-regexp="^(0[1-68])(?:[ _.-]?(\d{2})){4}$"
                           data-validation-error-msg="Veuillez fournir un n° de tel. valide"/>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label for="portable">Téléphone mobile</label> / <span
                            class="en">Mobile number</span>
                    <input type="tel" class="form-control" id="portable"
                           name="portable"
                           placeholder="Votre numéro" data-validation-optional="true"
                           data-validation="custom"
                           data-validation-regexp="^(0[1-68])(?:[ _.-]?(\d{2})){4}$"
                           data-validation-error-msg="Veuillez fournir un n° de tel. valide"/>
                </div>
            </div>
            <div class="col-md-4">
                <div class="form-group">
                    <label for="mail">Courriel</label> / <span
                            class="en">E-mail</span>
                    <span class="label label-danger">Requis</span>
                    <input type="email" class="form-control required" id="mail"
                           name="mail" placeholder="Votre mail"
                           data-validation="email"
                           data-validation-error-msg="Veuillez entrer un email valide"/>
                </div>
            </div>
        </div>
    </div>
</div>

