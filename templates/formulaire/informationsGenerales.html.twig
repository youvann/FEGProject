{% set mois = ["Janvier", "Février", "Mars", "Avril", "Mai", "Juin", "Juillet", "Août", "Septembre", "Octobre", "Novembre", "Décembre"] %}

<input type="hidden" id="autre" name="autre" value=""/>
{% if typeDossier == "CA" %}
    {% if dossierPdf.informationsPrealablesCandidature is not empty %}
        <div class="well">{{ dossierPdf.informationsPrealablesCandidature | raw }}</div>
    {% endif %}
{% else %}
    {% if dossierPdf.informationsPrealablesPreinscription is not empty %}
        <div class="well">{{ dossierPdf.informationsPrealablesPreinscription | raw }}</div>
    {% endif %}
{% endif %}
<div class="panel panel-primary">
    <div class="panel-heading">{% if typeDossier == "CA" %}Candidat <span class="en_cadre"> / Applicant</span>{% else %}Etudiant<span class="en_cadre"> / Student</span>{% endif %}
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <div class="radio">
                        <label for="titulaire_1">
                            <input type="radio" name="titulaire" id="titulaire_1"
                                   value="1" checked>
                            <b>{{ titulaire1 }}{% if dateTitulaire1 != "" %} - Veuillez remplir le formulaire et joindre les documents avant le {{ dateTitulaire1 }}
                            {% endif %}</b>
                        </label>
                    </div>
                    <div class="radio">
                        <label for="titulaire_2">
                            <input type="radio" name="titulaire" id="titulaire_2"
                                   value="2">
                            <b>{{ titulaire2 }}{% if dateTitulaire2 != "" %} - Veuillez remplir le formulaire et joindre les documents avant le {{ dateTitulaire2 }}
                                {% endif %}</b></label>
                    </div>
                    <div class="radio">
                        <label for="titulaire_3">
                            <input type="radio" name="titulaire" id="titulaire_3"
                                   value="3">
                            <b>{{ titulaire3 }}*{% if dateTitulaire3 != "" %} - Veuillez remplir le formulaire et joindre les documents avant le {{ dateTitulaire3 }}
                                {% endif %}</b>
                            </b>
                        </label>
                    </div>
                    <p class="help-block">
                        <i>* Dossier à utiliser si vous résidez dans l’Espace
                            européen, ou dans un pays où il
                            n’existe
                            pas
                            d’espace Campus-France (voir www.campusfrance.org). Tout
                            dossier contrevenant à cette
                            prescription
                            ne sera pas examiné.</i>
                    </p>
                </div>
            </div>
        </div>

        <hr/>
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <div class="radio">
                        <label>
